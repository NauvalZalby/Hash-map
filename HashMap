import java.util.HashMap;

public class Main {
    public static void main(String[] args) {
        // Membuat objek HashMap
        HashMap<String, Integer> umur = new HashMap<>();

        // Menambahkan pasangan kunci-nilai
        umur.put("Alice", 25);
        umur.put("Bob", 30);
        umur.put("Charlie", 28);

        // Mendapatkan nilai berdasarkan kunci "Alice"
        int umurAlice = umur.get("Alice");
        System.out.println("Umur Alice: " + umurAlice);
    }
}
